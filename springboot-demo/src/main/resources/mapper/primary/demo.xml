<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tim.dao.primary.DemoDao">
    <select id="select" resultType="java.util.Map">
        select id,en_name from emp
    </select>

    <delete id="delete">
        delete from emp where 1 = 1/0
    </delete>
    <update id="update">
        update emp set name = CONCAT(name,name)
    </update>
</mapper>